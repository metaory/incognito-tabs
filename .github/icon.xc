set d 0
set o 1
set r 50
set stroke-linejoin round
set stroke-linecap round
set ff sans
set fw bold
set fs 10em
set s 500
set w 96
set h 96
set r_x 2
set r_y 2
set border 6

set bg #CCEE44
set fg #220033
set stroke #220033

set t_transform rotate(16)
set t_dx 22
set t_dy -2

pre icons

set s 16
put 𐝕 icon-16.png

set s 48
put 𐝕 icon-48.png

set s 128
put 𐝕 icon-128.png

